
<% if(typeof results != 'undefined'){ %>
    
    <% var c = 1; %> 
    <table class="table">
        <thead>
            <tr>
              <th scope="col"># Factura</th>
              <th scope="col">TCEA</th>
              
              <th scope="col">Tasa descontada</th>
              <th scope="col">Valor Nominal</th>
              <th scope="col">Descuento</th>
              <th scope="col">Valor Neto</th>
            </tr>
        </thead>
        <tbody>
    <% results.forEach(function(result){ %>
            <tr>
                <th scope="row"><%= c; %></th>
                <td><%= (result.TCE*100).toFixed(4); %>%</td>
      
                <td><%= (result.tasaDes*100).toFixed(4); %>%</td>
                <td><%= result.valNom.toLocaleString('es-PE',{ style: 'currency', currency: 'PEN' }); %></td>
                <td><%= Number(result.des.toFixed(4)).toLocaleString('es-PE',{ style: 'currency', currency: 'PEN' }); %></td>
                <td><%= Number(result.valNeto.toFixed(4)).toLocaleString('es-PE',{ style: 'currency', currency: 'PEN' }); %></td>
            </tr>  
            <% c = c+1; %> 
    <% }); %>
        </tbody>
    </table>    
      
<% } %>